  http://go.microsoft.com/fwlink/?LinkId=169433
  -->

<configuration>
    <connectionStrings>
        <add name="TNSRTCConnectionString" connectionString="Data Source=localhost;Initial Catalog=TNSRTC;Persist Security Info=True;User ID=sa;Password=123"
            providerName="System.Data.SqlClient" />
    </connectionStrings>
    <system.web>
        <compilation debug="true" targetFramework="4.0" />
      
		<authentication mode="Forms">
			<forms defaultUrl="user/WelcomeUser.aspx" loginUrl="LoginPage.aspx" name="TNSRTCcookie" ></forms>
		</authentication>
		<authorization>
			<allow users="*"/>
		</authorization>
    </system.web>

	<location path="user">
		<system.web>
			<authorization>
				<deny users="?"/>
			</authorization>
		</system.web>
	</location>


</configuration>
